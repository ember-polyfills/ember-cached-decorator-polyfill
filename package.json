{
  "name": "ember-cached-decorator-polyfill",
  "version": "1.0.1",
  "description": "Polyfill for RFC 566: @cached",
  "keywords": [
    "ember-addon",
    "polyfill",
    "glimmer",
    "tracking",
    "cache",
    "cached",
    "memo",
    "memoize",
    "memoized",
    "memoization"
  ],
  "exports": {
    ".": "./index.js",
    "./babel-plugin": "./lib/transpile-modules.js"
  },
  "homepage": "https://github.com/ember-polyfills/ember-cached-decorator-polyfill#readme",
  "bugs": {
    "url": "https://github.com/ember-polyfills/ember-cached-decorator-polyfill/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ember-polyfills/ember-cached-decorator-polyfill.git"
  },
  "license": "MIT",
  "author": "Jan Buscht√∂ns <jan@buschtoens.me> (https://jan.buschtoens.me)",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "build:node": "tsc -p tsconfig.node.json",
    "prepare": "yarn run build:node",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel \"lint:!(fix)\"",
    "lint:fix": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*:fix",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:types": "tsc --noEmit",
    "lint:js:fix": "eslint . --fix",
    "start": "ember serve",
    "test": "npm-run-all lint test:*",
    "test:ember": "ember test",
    "test:ember-compatibility": "ember try:each"
  },
  "dependencies": {
    "@embroider/macros": "^1.8.3",
    "@glimmer/tracking": "^1.1.2",
    "babel-import-util": "^1.2.2",
    "ember-cache-primitive-polyfill": "^1.0.1",
    "ember-cli-babel": "^7.26.11",
    "ember-cli-babel-plugin-helpers": "^1.1.1"
  },
  "devDependencies": {
    "@ember/optional-features": "^2.0.0",
    "@ember/test-helpers": "^2.8.1",
    "@embroider/test-setup": "^1.8.3",
    "@glimmer/component": "^1.1.2",
    "@types/babel__core": "^7.1.19",
    "@types/babel__traverse": "^7.18.1",
    "@types/ember": "^4.0.1",
    "@types/ember-qunit": "^5.0.1",
    "@types/ember-resolver": "^5.0.9",
    "@types/ember__test-helpers": "^2.8.1",
    "@types/qunit": "^2.19.2",
    "@types/rsvp": "^4.0.3",
    "@typescript-eslint/eslint-plugin": "^5.37.0",
    "@typescript-eslint/parser": "^5.37.0",
    "broccoli-asset-rev": "^3.0.0",
    "ember-auto-import": "^2.4.2",
    "ember-cli": "~4.7.0",
    "ember-cli-dependency-checker": "^3.3.1",
    "ember-cli-htmlbars": "^6.1.0",
    "ember-cli-inject-live-reload": "^2.1.0",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-terser": "^4.0.2",
    "ember-cli-typescript": "^3.1.4",
    "ember-cli-typescript-blueprints": "^3.0.0",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-load-initializers": "^2.1.2",
    "ember-page-title": "^7.0.0",
    "ember-qunit": "^5.1.5",
    "ember-resolver": "^8.0.3",
    "ember-source": "~4.7.0",
    "ember-source-channel-url": "^3.0.0",
    "ember-template-lint": "^4.14.0",
    "ember-try": "^2.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-ember": "^11.0.6",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-qunit": "^7.3.1",
    "loader.js": "^4.7.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "qunit": "^2.19.1",
    "qunit-dom": "^2.0.0",
    "release-it": "^15.5.0",
    "release-it-lerna-changelog": "^5.0.0",
    "typescript": "~4.8.3",
    "webpack": "^5.74.0"
  },
  "engines": {
    "node": "14.* || >= 16"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "before": "ember-cli-babel",
    "configPath": "tests/dummy/config"
  },
  "peerDependencies": {
    "ember-source": "^3.13.0 || ^4.0.0"
  },
  "release-it": {
    "plugins": {
      "release-it-lerna-changelog": {
        "infile": "CHANGELOG.md",
        "launchEditor": false
      }
    },
    "git": {
      "tagName": "v${version}"
    },
    "github": {
      "release": true,
      "tokenRef": "GITHUB_AUTH"
    }
  }
}
